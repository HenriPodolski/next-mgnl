{
  "name": "web",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "test": "",
    "dev": "npx next dev",
    "dev:preview": "cross-env MGNL_PREVIEW=true concurrently 'npm run dev' 'npm run symlink:preview' 'npm run download:preview'",
    "download:preview": "npx wget http://localhost:3000/Home -d ./build/Home.html && npx wget http://localhost:3000/Home/Test -d ./build/Home/Test.html",
    "symlink:preview": "npx mkdirp ./build && env-cmd --no-override -f .env cross-env-shell 'npx symlink-dir ./build $MGNL_RESOURCES_DIR'",
    "build": "npx next build",
    "build:preview": "cross-env MGNL_PREVIEW=true npx next build",
    "start": "npx next start",
    "lint": "npx next lint",
    "export": "cross-env-shell MGNL_PREVIEW=true 'npm run build && npx next export'",
    "export:preview": "npm run build:preview && env-cmd --no-override -f .env cross-env-shell MGNL_PREVIEW_EXPORT=true MGNL_PREVIEW=true 'npx next export -o $MGNL_RESOURCES_DIR'"
  },
  "dependencies": {
    "@magnolia/react-editor": "^1.1.0-beta2",
    "cors": "^2.8.5",
    "next": "^11.1.2",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.1",
    "next-secure-headers": "^2.2.0",
    "next-transpile-modules": "^8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "swr": "^1.0.1"
  },
  "resolutions": {
    "react": "17.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/react": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "autoprefixer": "^10.3.4",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mkdirp": "^1.0.4",
    "node-wget": "^0.4.3",
    "postcss": "^8.3.6",
    "prettier": "^2.4.0",
    "sass": "^1.38.1",
    "symlink-dir": "^5.0.1",
    "tailwindcss": "^2.2.15"
  },
  "engines": {
    "node": ">=16.4.2",
    "npm": ">=7.0.0"
  }
}
