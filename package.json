{
  "name": "next-mgnl",
  "version": "0.0.2",
  "private": true,
  "description": "",
  "scripts": {
    "start": "npm run start:web",
    "start:web": "npm run start -w @next-mgnl/web",
    "build:web": "npm run build -w @next-mgnl/web",
    "build:web:preview": "npm run build:preview -w @next-mgnl/web",
    "test": "npm run test --workspaces",
    "dev": "npx concurrently \"npm:dev:lib\" \"npm:dev:web\"",
    "dev:preview": "npx concurrently \"npm:dev:lib\" \"npm:dev:web:preview\"",
    "dev:web": "npm run dev -w @next-mgnl/web",
    "dev:web:preview": "npm run dev:preview -w @next-mgnl/web",
    "dev:lib": "npm run dev -w @next-mgnl/lib"
  },
  "author": "",
  "license": "MIT",
  "workspaces": [
    "./packages/lib",
    "./packages/web"
  ],
  "devDependencies": {
    "concurrently": "^6.2.1"
  },
  "engines": {
    "node": ">=16.4.2",
    "npm": ">=7.0.0"
  }
}
